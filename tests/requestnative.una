define
    "Request test with bridge to native alert"

use "acuna/kernel"
use "dojo/request" "drequest"
use "dojo/when" "dwhen"
use "acuna/kernel/native!alert" "nalert"

request "test.una" then
    alert
    alert


define "request" 1

bridge
    1 drequest


define "then" 3

bridge
    3 dwhen


define "alert" 1

bridge
    1 nalert
